{
    "name": "vscode-open-in-sourcetree",
    "displayName": "Open in SourceTree",
    "description": "Open project in SourceTree",
    "icon": "1024x1024.png",
    "version": "0.2.4",
    "publisher": "morrislaptop",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:openInSourceTree.open"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "openInSourceTree.open",
                "title": "Open in SourceTree"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "openInSourceTree.open"
                }
            ],
            "editor/title/context": [
                {
                    "command": "openInSourceTree.open"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.0",
        "eslint": "^4.6.1",
        "mocha": "^3.5.0",
        "typescript": "^2.5.2",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "open": "0.0.5"
    }
}